
import java.util.Arrays;

public class RawByRawSummation {

	public static void main(String[] args) {
		
		int[][] integers = {
				{1,2,3,4},
				{5,6,7,8},
				{9,7,5,4}
		};
		
		for (int i=0;i<3;i++)
		{
			int sum = 0;
	    for(int j=0;j<4;j++) {
	    	
	    	sum+=integers[i][j];
	    	
	    }
	    System.out.println("Summation & Number of Row  "+(i+1)+" : "+sum);
		}
		
		//System.out.println("Summation & Number of Row : (i+1) "+sum);
	}

}
